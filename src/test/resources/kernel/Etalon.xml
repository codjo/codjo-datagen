    <entity table="AP_MEI_A" name="net.codjo.pims.data.MeiRealManagA">
        <feature>
            <doc-structure/>
            <sql pk-generator="no" name="AP_MEI_A"/>
            <sql-index>
                <idx name-prefix="X1_" type="primary-key"/>
            </sql-index>
            <bean/>
            <castor/>
            <handler-delete id="deleteMeiRealManagA"/>
